    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/package": "2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.13.1",
    "happy-dom": "^20.0.11",
    "mdsvex": "^0.12.6",
    "mdsvexamples": "^0.5.0",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3",
    "svelte-toc": "^0.6.2",
    "svelte2tsx": "^0.7.46",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "svelte",
    "multi-select",
    "single-select",
    "form",
    "input"
