{
  "name": "svelte-multiselect",
  "description": "Svelte multi-select component",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://svelte-multiselect.netlify.app",
  "repository": "https://github.com/janosh/svelte-multiselect",
  "license": "MIT",
  "version": "1.1.8",
  "type": "module",
  "bugs": {
    "url": "https://github.com/janosh/svelte-multiselect/issues"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "serve": "yarn build && yarn preview",
    "prebuild": "cp readme.md src/docs.svx && sed -i.bak '1,/^<!-- remove above in docs -->$/d' src/docs.svx && rm src/docs.svx.bak",
    "predev": "yarn prebuild",
    "release": "yarn version && svelte-kit package && yarn publish package"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.115",
    "eslint": "^7.29.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "hastscript": "^7.0.1",
    "mdsvex": "^0.9.7",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.3.1",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "svelte": "^3.38.2",
    "svelte-toc": "^0.1.5",
    "vite": "^2.3.8"
  },
  "keywords": [
    "svelte",
    "multi-select",
    "single-select",
    "form",
    "input"
  ],
  "publishConfig": {
    "access": "public"
  }
}